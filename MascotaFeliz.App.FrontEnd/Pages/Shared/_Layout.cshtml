<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MascotaFeliz.App.FrontEnd</title>
    <!--<<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />-->
    <!--<<link rel="stylesheet" href="~/css/site.css" />-->
    <link rel="stylesheet" href="~/css/style.css" />
    
</head>
<body>
    <header>
    <div class="logo-enlaces">
                <a asp-area="" asp-page="/Index" class="logo"></a>
        <nav>
                <a asp-area="" asp-page="/Mascota/ListarMascota">Lista Mascotas</a>
                <a asp-area="" asp-page="/Dueno/ListarDuenos">Lista Dueños</a>
                <a asp-area="" asp-page="/Veterinario/ListarVeterinario">Lista Veterinarios</a>         
        </nav>
    </div>
    </header>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

<footer class="pie-pagina">
    <div>Derechos Reservados | G5_Tech</div>    
</footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
